generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
}


model contactSubmission {
  id                 String         @id @default(cuid())
  name               String         
  email              String         @unique
  phone              String         
  message            String?  
  createdAt          DateTime        @default(now())
}