generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
    referentialIntegrity = "prisma"
}


model contactSubmission {
  id                 Int              @default(autoincrement()) @id
  createdAt          DateTime         @default(now())
  name               String         
  email              String        @unique
  phone              String         
  message            String?  
}